  record GCVc GCr;
  record GCVc oldGCr;
  record GCSVc GCSr;
  record GCSVc oldGCSr;
  record GCRVc GCRr;
  
  if (CompanyProgramMode==kProgramModeLiveSync) then begin
    goto LIVCashVcCreateSoldGiftCert_Invalidate;
