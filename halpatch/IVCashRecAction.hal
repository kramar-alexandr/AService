function string 50 GetTimeStampAndUser(string MachineCode)
BEGIN
  date curdate;
  time curtime;
  string 10 mth,day,hour,min,sec;
  
  curdate = CurrentDate;
  if (GetMonth(curdate)<10) then begin
  	mth = "0" & GetMonth(curdate);
  end else begin
  	mth = "" & GetMonth(curdate);	
  end;
  if (GetDay(curdate)<10) then begin
  	day = "0" & GetDay(curdate);
  end else begin
  	day = "" & GetDay(curdate);
  end;
  
  curtime = CurrentTime;
  if (GetHour(curtime)<10) then begin
  	hour = "0" & GetHour(curtime);
  end else begin
  	hour = "" & GetHour(curtime);
  end;
  if (GetMinute(curtime)<10) then begin
  	min = "0" & GetMinute(curtime);
  end else begin
  	min = "" & GetMinute(curtime);
  end;
  if (GetSecond(curtime)<10) then begin
  	sec = "0" & GetSecond(curtime);
  end else begin
  	sec = "" & GetSecond(curtime);
  end;
  
  if (blank(MachineCode)) then begin
  	GetTimeStampAndUser = "";
  end else begin
  	GetTimeStampAndUser = GetYear(curdate) & mth & day & hour & min & sec & MachineCode;
  end;
  RETURN;
END; //Edit***************************Sasha2,13:57 24.09.2014 }


